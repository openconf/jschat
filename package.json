{
  "name": "angular-app-server",
  "description": "Back end server to support our angular app",
  "version": "0.0.1",

  "private": true,
  "scripts": {
    "start": "node server.js",
    "test": "npm run setup; npm run compile; node ./node_modules/grunt-cli/bin/grunt;",
    "t": "node ./node_modules/grunt-cli/bin/grunt",
    "setup": "cd tests/socketClientPatched && npm install && cd ../../ && node ./node_modules/component/bin/component install",
    "client": "node ./node_modules/brunch/bin/brunch build",
    "watch": "node ./node_modules/brunch/bin/brunch watch --server",
    "build-app": "node ./node_modules/brunch/bin/brunch build;node node_modules/grunt-cli/bin/grunt buildapp --dest=./desktop",
    "package": "node node_modules/grunt-cli/bin/grunt compress:mac compress:win compress:linux32 compress:linux64 --src=./desktop --dest=../desktop"
  },
  "dependencies": {
    "express": "~3.4.8",
    "passport": "~0.1.12",
    "nconf": "latest",
    "secure.me": "~0.1.4",
    "underscore": "~1.5.1",
    "async": "~0.2.9",
    "passport-github": "~0.1.5",
    "engine.io": "~0.8.0",
    "engine.io-client": "~0.8.0",
    "grunt-mocha-test": "~0.8.2",
    "nock": "~0.27.1",
    "socker": "~0.1.6",
    "engine.io-session": "0.0.4",
    "connect": "~2.12.0",
    "redis": "~0.10.0",
    "parse-redis-url": "0.0.1",
    "connect-redis": "~1.4.6",
    "grunt-node-webkit-builder": "~0.2.2",
    "grunt-contrib-copy": "~0.5.0",
    "grunt-contrib-compress": "~0.9.1",
    "grunt": "~0.4.1",
    "grunt-cli": "~0.1.9",

    "less-brunch": "~1.7.2",
    "react-brunch": "~1.0.6",
    "auto-reload-brunch": "~1.7.3",
    "component": "~0.19.9",
    "brunch": "git+https://git@github.com/edjafarov/brunch.git"
  },
  "devDependencies": {
    "mocha": "~1.12.1",
    "chai": "~1.7.2",
    "request": "~2.27.0",
    "ws": "~0.4.31",
    "rewire": "~2.0.0"
  }
}
